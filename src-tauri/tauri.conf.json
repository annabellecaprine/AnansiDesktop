{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/core/tauri-schema.json",
  "productName": "Anansi",
  "mainBinaryName": "anansi",
  "version": "1.0.0",
  "identifier": "com.anansi.desktop",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../ui"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Anansi",
        "width": 1280,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}